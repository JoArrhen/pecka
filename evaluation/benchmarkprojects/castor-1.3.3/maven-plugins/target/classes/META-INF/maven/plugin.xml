<?xml version="1.0" encoding="UTF-8"?>
<plugin>
  <name>Castor XML - Maven Mojos for executing XMLCTF tests</name>
  <description>The Codehaus is a collaborative environment for building open source projects with a strong emphasis on modern languages, focussed on quality components that meet real world needs.</description>
  <groupId>org.codehaus.castor</groupId>
  <artifactId>castor-maven-plugins</artifactId>
  <version>1.3.3-rc2-SNAPSHOT</version>
  <goalPrefix>castors</goalPrefix>
  <isolatedRealm>false</isolatedRealm>
  <inheritedByDefault>true</inheritedByDefault>
  <mojos>
    <mojo>
      <goal>xmlctf-text</goal>
      <description>Uses a simple junit.ui.TestRunner TextRunner to run the XMLCTF test suite.</description>
      <requiresDependencyResolution>runtime</requiresDependencyResolution>
      <requiresDirectInvocation>false</requiresDirectInvocation>
      <requiresProject>true</requiresProject>
      <requiresReports>false</requiresReports>
      <aggregator>false</aggregator>
      <requiresOnline>false</requiresOnline>
      <inheritedByDefault>true</inheritedByDefault>
      <implementation>org.codehaus.castor.maven.xmlctf.TextTestSuiteMojo</implementation>
      <language>java</language>
      <instantiationStrategy>per-lookup</instantiationStrategy>
      <executionStrategy>once-per-session</executionStrategy>
      <threadSafe>false</threadSafe>
      <parameters>
        <parameter>
          <name>outputRoot</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>Target directory used for the testclasses.</description>
        </parameter>
        <parameter>
          <name>pathToTools</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>Optional parameter to overwrite the absolute path to the java tools.jar</description>
        </parameter>
        <parameter>
          <name>project</name>
          <type>org.apache.maven.project.MavenProject</type>
          <required>true</required>
          <editable>true</editable>
          <description>The project whose project files to create.</description>
        </parameter>
        <parameter>
          <name>testRoot</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>The source directory of the tests.</description>
        </parameter>
      </parameters>
      <configuration>
        <outputRoot implementation="java.lang.String">./target/xmlctf</outputRoot>
        <project implementation="org.apache.maven.project.MavenProject">${project}</project>
      </configuration>
    </mojo>
  </mojos>
